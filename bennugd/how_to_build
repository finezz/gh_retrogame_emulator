$ export TARGETMACH=mipsel-openwrt-linux
$ export BUILDMACH=i686-pc-linux-gnu
$ export CROSS=mipsel-openwrt-linux
$ export CC=${CROSS}-gcc
$ export LD=${CROSS}-ld
$ export AS=${CROSS}-as
$ export CXX=${CROSS}-g++

$ cd core
$ ./configure --host=$TARGETMACH

$ cd modules
$ ./configure --host=$TARGETMACH

$ cd tools/moddesc 
$ ./configure --host=$TARGETMACH
